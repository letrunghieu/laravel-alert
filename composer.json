{
	"name": "hieu-le/laravel-alert",
	"description": "Laravel global site message system",
	"keywords": ["bootstrap", "laravel", "message", "global", "alert"],
	"license": "MIT",
	"authors": [
		{
			"name": "Hieu Le",
			"email": "letrunghieu.cse09@gmail.com"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"illuminate/support": "^5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"orchestra/testbench": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"HieuLe\\Alert\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"HieuLe\\Alert\\AlertServiceProvider"
			],
			"aliases": {
				"Alert": "HieuLe\\Alert\\Facades\\Alert"
			}
		}
	}
}
